<div class="row">
    <div class="col-md-8">
        <div id="portfolioItemCarrousel" class="owl-carousel owl-theme">
            <% var images=getCurrentImages(); %>
                <% for (var i=0 ; i < images.length; i++) { %>
                    <div class="item">
                        <img src='<%= images[i] %>'>
                    </div>
                    <% } %>
        </div>
    </div>
    <div class="col-md-4 item-description"> 
        <h2>
            <%= title %>
        </h2>
        <%- yield %>
    </div>
</div>